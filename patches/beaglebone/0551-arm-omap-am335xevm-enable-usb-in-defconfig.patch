-CONFIG_USB_DEBUG=y
 CONFIG_USB_ANNOUNCE_NEW_DEVICES=y
 CONFIG_USB_DEVICEFS=y
-CONFIG_USB_SUSPEND=y
-CONFIG_USB_MON=y
-CONFIG_USB_WDM=y
+CONFIG_USB_MUSB_HDRC=y
+CONFIG_USB_MUSB_TI81XX_GLUE=y
+CONFIG_USB_MUSB_TI81XX=y
